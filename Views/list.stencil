{% extends "master.stencil" %}

{% block title %}List{% endblock %}

{% block body %}
<h1>File List</h1>
{% for file in files %}
<a
    href="/originals/{{ file }}"
    target="_blank"
/>{{ file }}</a>
{% empty %}
<p>No files uploaded</p>
{% endfor %}

<form action="/upload"
    enctype="multipart/form-data"
    method="POST">
<p><input type="file" name="upload" multiple/></p>
<p><button type="submit" class="btn">Upload</button></p>
</form>
{% endblock %}
